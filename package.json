{
  "name": "wikibase-console",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "command line app to explore wikibase",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "start": "node ./dist/main.js",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "test": "vitest",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "keywords": [
    "wikibase",
    "wikipedia",
    "wikimedia"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^1.9.0",
    "inquirer": "^12.6.1",
    "prompts": "^2.4.2",
    "wikibase-sdk": "^10.2.3"
  },
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/node": "^22.15.17",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "3.1.3",
    "eslint": "^9.27.0",
    "eslint-plugin-gb": "^9.1.0",
    "globals": "^16.1.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3"
  }
}
